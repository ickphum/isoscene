                                            <orient>wxVERTICAL</orient>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_LEFT|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="a5_rbn">
                                                    <style>wxRB_GROUP</style>
                                                    <label>A5</label>
                                                    <value>0</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_LEFT|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="a4_rbn">
                                                    <label>A4</label>
                                                    <value>0</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="a3_rbn">
                                                    <label>A3</label>
                                                    <value>0</value>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_TOP|wxTOP|wxBOTTOM</flag>
                                        <border>10</border>
                                        <object class="wxStaticText" name="wxID_STATIC">
